<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.hy.dynamicdatasource.mapper.admin.UserMapper">
    <insert id="insertUser">
        INSERT INTO `user`(
            `name`,
            `password`
        ) VALUES (
            #{name},
            #{password}
        )
    </insert>

    <insert id="selectUser">
        INSERT INTO `user`(
            `name`,
            `password`
        ) VALUES (
            #{name},
            #{password}
        )
    </insert>
</mapper>